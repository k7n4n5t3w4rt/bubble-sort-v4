{
  "name": "bubblesort",
  "description": "BubbleSort visualization built with Preact and Three.js on the GoodThing boilerplate",
  "version": "1.0.0",
  "author": "Kynan Stewart",
  "bugs": {
    "url": "https://github.com/k7n4n5t3w4rt/goodthing/issues"
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "finalhandler": "^1.1.2",
    "history": "^4.10.1",
    "htm": "^3.0.4",
    "immer": "^11.0.1",
    "preact": "^10.4.1",
    "preact-router": "^3.2.1",
    "rxjs": "^7.4.0",
    "serve-static": "^1.14.1",
    "simplestyle-js": "^2.3.0-beta.0",
    "three": "^0.136.0"
  },
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "cypress": "^14.5.4",
    "esinstall": "^1.0.4",
    "faucet": "^0.0.4",
    "fixpack": "^3.0.6",
    "flow-bin": "^0.156.0",
    "glob": "^7.1.6",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.0",
    "should": "^13.2.3",
    "wait-on": "^5.1.0"
  },
  "scripts": {
    "browsersync": "browser-sync start --server '.' --files 'css/**/*.css, js/**/*.js, web_modules/**/*.js, index.html'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "esinstall": "node esinstall.js",
    "fixpack": "fixpack",
    "flow": "flow check",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "goodthing:update": "rsync -a --exclude='.git' --exclude='docs' --exclude='web_modules' ../goodthing/ ./",
    "lint": "prettier --check \"src/**/*.{js,jsx}\"",
    "start": "npm run browsersync",
    "test": "npm run testy && npm run cypress:run",
    "testy": "node --no-warnings server/testyrunner.js | faucet"
  },
  "type": "module"
}

